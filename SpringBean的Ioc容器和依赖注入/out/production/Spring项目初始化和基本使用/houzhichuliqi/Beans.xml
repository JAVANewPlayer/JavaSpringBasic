<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="helloWorld" class="houzhichuliqi.HelloWorld" init-method="init" destroy-method="destroy">
        <property name="message" value="隔壁老黄"></property>
    </bean>
    <bean class="houzhichuliqi.InitHelloWorld"></bean>
    <bean class="houzhichuliqi.InitHelloWorldTwo"></bean>
</beans>